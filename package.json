{
  "name": "less-admin",
  "version": "0.1.0",
  "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "author": "Maslow <wangfugen@126.com>",
  "scripts": {
    "deploy:win": "scp -rv ./dist/  zhuo-dev:/home/go-chess/admin",
    "deploy": "rsync -rvtP ./dist/  zhuo-dev:/home/go-chess/admin",
    "dev": "vue-cli-service serve",
    "lint": "eslint --ext .js,.vue src",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "new": "plop",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "prepare": "husky install",
    "commit": "commit"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "clipboard": "2.0.4",
    "element-ui": "2.13.2",
    "file-saver": "2.0.1",
    "js-cookie": "2.2.0",
    "jszip": "3.2.1",
    "less-api-client": "^0.8.4",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.25.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.2.0",
    "vue": "2.6.10",
    "vue-router": "3.0.2",
    "vuex": "3.1.0",
    "xlsx": "0.16.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/prompt-cli": "^12.0.1",
    "@types/lodash": "^4.14.168",
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "4.4.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "^5.2.0",
    "lint-staged": "8.1.5",
    "mockjs": "1.0.1-beta3",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "plop": "2.3.0",
    "runjs": "^4.4.2",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "serve-static": "1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-template-compiler": "2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
  },
  "engines": {
    "node": ">=14",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "vue",
    "admin",
    "less-framework",
    "element-ui",
    "boilerplate",
    "admin-template",
    "less-api"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maslow/less-admin.git"
  }
}
